<Window x:Class="PL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="MainWindow" Height="640" Width="800">
    <Grid>
        <Button Name="Cart" Content="Cart" HorizontalAlignment="Left" Margin="550,170,0,0" VerticalAlignment="Top" Click="ShowCartsButton_Click" Height="45" Width="90"/>
        <Button Name="Order" Content="Order" HorizontalAlignment="Left" Margin="350,170,0,0" VerticalAlignment="Top" Click="ShowOrdersButton_Click" Height="45" Width="90"/>
        <Button Name="Product" Content="Product" HorizontalAlignment="Left" Margin="150,170,0,0" VerticalAlignment="Top" Click="ShowProductsButton_Click" Height="45" Width="90"/>
    </Grid>
</Window>
